<form method="post" action="{{ path('equipo_delete', {'id': equipo.id}) }}" onsubmit="return confirm('Estas seguro?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ equipo.id) }}">
    <button class="btn">Borrar</button>
</form>
