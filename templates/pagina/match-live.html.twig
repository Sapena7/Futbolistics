                    {% endif %}
                    {% if jugador.dorsal == 7 %}
                        <a href="player-second-page.html" class="player bottom lf  " data-toggle="tooltip" data-placement="bottom" title="" data-original-title="{{ jugador.nombre }}">
                            <img width="56" height="56" src="{{ asset('assets/images/common/member-avatar.jpg')}}"  alt="player" >                           {{ jugador.dorsal }}                       </a>
                    {% endif %}
                    {% if jugador.dorsal == 8 %}
