                    <div class="match-lineup">
    <div class="no-gutter">
        <div class="col-sm-6">
            <div class="head">
                <div class="name">{{ partido.EquipoLocal.nombre }}</div>
            </div>
            <div class="team-wrap">
                    {% for jugador in jugadoresLocal %}
                        {% if jugador.dorsal >= 1 and jugador.dorsal <=10 or jugador.dorsal == 17 %}
                            <div class="member">
                                <a href="../jugador/player.html.twig" class="avatar"><img src="{{ asset('assets/images/common/member-avatar.jpg')}}" alt="member-avatar"></a>
                                <div class="info">
                                    <div class="wrap">
                                        <a href="../jugador/player.html.twig" class="name">{{ jugador.nombre }}</a><br>
                                        <div class="card yellow" data-toggle="tooltip" data-placement="top" title="" data-original-title="32’"></div>
                                        <div class="goal" data-toggle="tooltip" data-placement="top" title="" data-original-title="32’"><img src="{{ asset('assets/images/common/timeline-goal.jpg')}}" alt="timeline-goal"></div>
                                    </div>
                                </div>
                            </div>
                        {% endif %}
                    {% endfor %}
            </div>
        </div>
        <div class="col-sm-6">
            <div class="head">
                <div class="name right">{{ partido.EquipoVisitante.nombre }}</div>
            </div>
                <div class="team-wrap right">

                    {% for jugador in jugadoresVisitante %}
                        {% if jugador.dorsal >= 1 and jugador.dorsal <=10 or jugador.dorsal == 17 %}
                            <div class="member">
                                <div class="info">
                                    <div class="wrap">
                                        <a href="../jugador/player.html.twig" class="name">{{ jugador.nombre }}</a><br>
                                        <div class="card yellow" data-toggle="tooltip" data-placement="top" title="" data-original-title="32’"></div>
                                        <div class="goal" data-toggle="tooltip" data-placement="top" title="" data-original-title="32’"><img src="{{ asset('assets/images/common/timeline-goal.jpg')}}" alt="timeline-goal"></div>
                                    </div>
                                </div>
                                <a href="../jugador/player.html.twig" class="avatar"><img src="{{ asset('assets/images/common/member-avatar.jpg')}}" alt="member-avatar"></a>
                            </div>
                        {% endif %}
                    {% endfor %}

                </div>
            </div>
        </div>					
    </div>
                    <h3>Entrenadores</h3>
                    
<div class="match-lineup">
    <div class="no-gutter">
        <div class="col-sm-6">
            <div class="team-wrap">	
                <div class="member">
                    <a href="../jugador/player.html.twig" class="avatar"><img src="{{ asset('assets/images/soccer/coach.png')}}" alt="member-avatar"></a>
                    <div class="info">
                        <div class="wrap">
                            <a href="../jugador/player.html.twig" class="name">{{ partido.EquipoLocal.entrenador }}</a>
                        </div>
                    </div>
                </div>
            </div>	
        </div>
        <div class="col-sm-6">
            <div class="team-wrap right">	
                <div class="member">
                    <div class="info">
                        <div class="wrap">
                            <a href="../jugador/player.html.twig" class="name">{{ partido.EquipoVisitante.entrenador }} </a>
                        </div>
                    </div>
                    <a href="../jugador/player.html.twig" class="avatar"><img src="{{ asset('assets/images/soccer/coach.png')}}" alt="member-avatar"></a>
                </div>
            </div>
        </div>
    </div>
</div>
                </div>		
                <div class="col-lg-6">
                    <h3>Estadisticas</h3>
                    
<div class="match-stat-table">
    <div class="item">
        <div class="name">Posesión</div>

        {% set posesionLocal = random(50, 75) %}
        {% set posesionVisitante = 100 - posesionLocal %}
        <div class="percent">{{ posesionLocal }}</div>
        <div class="percent right">{{ posesionVisitante }}</div>
        <div class="percent-bar">
            <div class="bar bar-1" ></div>
        </div>
    </div>
    <div class="item">
        <div class="name">Corners</div>
        <div class="percent">{{ random(1, 10) }}</div>
        <div class="percent right">{{ random(1, 10) }}</div>
        <div class="percent-bar">
            <div class="bar bar-2" ></div>
        </div>
    </div>
    <div class="item">
        <div class="name">Faltas</div>
        <div class="percent">{{ random(1, 15) }}</div>
        <div class="percent right">{{ random(1, 15) }}</div>
        <div class="percent-bar">
            <div class="bar bar-3" ></div>
        </div>
    </div>
    <div class="item">
        <div class="name">Fueras de juego</div>
        <div class="percent">{{ random(1, 5) }}</div>
        <div class="percent right">{{ random(1, 5) }}</div>
        <div class="percent-bar">
            <div class="bar bar-4" ></div>
        </div>
    </div>
</div>

                    <h3>Estadio</h3>
                    <div class="arena">
    <div class="img-wrap">
        <img src="{{ asset('assets/images/estadios/')}}{{ partido.equipoLocal.estadio.fotoEstadio }}" width="300px" height="200px" alt="arena-img">
    </div>
    <div class="arena-info">
        <div class="name">{{ partido.estadio.nombre }} ({{ partido.estadio.ciudad }})</div>
        <div class="date">Fecha: {{ partido.fecha|date("d/m/Y") }}</div>
    </div>
</div>

                    <h3>Árbitros</h3>
                    
<div class="match-lineup">
    <div class="team-wrap">	
        <div class="member">
            <a href="../jugador/player.html.twig" class="avatar"><img src="{{ asset('assets/images/arbitros/Collina.jpg')}}" alt="member-avata"></a>
            <div class="info">
                <div class="wrap">
                    <a href="../jugador/player.html.twig" class="name">Pierluigi Collina</a>
                </div>
                <div class="country">Italia</div>
            </div>
        </div>
    </div>
    <div class="no-gutter">
        <div class="col-md-6">
            <div class="team-wrap">	
                <div class="member">
                    <a href="../jugador/player.html.twig" class="avatar"><img src="{{ asset('assets/images/arbitros/HowardWebb.jpg')}}" alt="member-avatar"></a>
                    <div class="info">
                        <div class="wrap">
                            <a href="../jugador/player.html.twig" class="name">Howard Webb</a>
                        </div>
                        <div class="country">Reino Unido</div>
                    </div>
                </div>
            </div>	
        </div>
        <div class="col-md-6">
            <div class="team-wrap">	
                <div class="member">
                    <a href="../jugador/player.html.twig" class="avatar"><img src="{{ asset('assets/images/arbitros/MarkClattenburg.jpg')}}" alt="member-avatar"></a>
                    <div class="info">
                        <div class="wrap">
                            <a href="../jugador/player.html.twig" class="name">Mark Clattenburg</a>
                        </div>
                        <div class="country">Reino Unido</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
                    <h3>Resumen</h3>
                    <div class="live-details">
                        {% if partido.golesLocal != partido.golesVisitante %}
                        Partido de la <b>{{ partido.jornada.jornada }}</b> disputado entre <b>{{ partido.equipoLocal.nombre }}</b> y <b>{{ partido.equipoVisitante.nombre }}</b> en el
                        estadio <b>{{ partido.estadio.nombre }}</b> de <b>{{ partido.estadio.ciudad }}</b> dónde los
                            {% if partido.golesLocal > partido.golesVisitante %}
                            locales
                            {% endif %}
                            {% if partido.golesLocal < partido.golesVisitante %}
                            visitantes
                            {% endif %}
                        se llevarón la victoria por <b>{{ partido.golesLocal }} - {{ partido.golesVisitante }}</b>
                        {% endif %}
                    </div>

                </div>
            </div>
        </div>	
    </section>

    <!--LIVE MATCH STAT END-->


