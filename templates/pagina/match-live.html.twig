            <div class="text">
                {{ partido.EquipoVisitante.nombre }}
            </div>
            <div class="avatar">
                <img src="{{ asset('assets/images/escudos/')}}{{ partido.equipoVisitante.fotoPerfil }}" alt="team-logo">   </div>
