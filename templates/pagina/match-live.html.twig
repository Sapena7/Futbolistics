    <ul class="team-legend" role="tablist">
        <li class="active">
            <a href="#football_pitch_top" data-toggle="tab" aria-controls="football_pitch_top" aria-expanded="true">
                <div class="circle red"></div>{{ partido.EquipoLocal.nombre }}             </a>
        </li>
        <li class="">
            <a href="#football_pitch_button" data-toggle="tab" aria-controls="football_pitch_button" aria-expanded="false">
